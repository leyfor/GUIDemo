<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <HBox prefHeight="220.0" prefWidth="420.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="CheckBox Objects">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperon" />
                  <CheckBox fx:id="beconCheckBox" mnemonicParsing="false" text="Becon" />
                  <CheckBox fx:id="pineAppleCheckBox" mnemonicParsing="false" text="PineApple" />
                  <Button mnemonicParsing="false" onAction="#pizzaOrderButtonPush" text="Order Pizza" />
                  <Label fx:id="label" alignment="TOP_LEFT" prefHeight="150.0" prefWidth="177.0" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="257.0" prefWidth="168.0" spacing="5.0">
               <children>
                  <Label text="ChoiceBox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPushed" text="Favourite Fruit" />
                  <Label fx:id="choiceBoxLabel" text="Label" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
